<div class="container px-5 px-sm-0">
  <div class="row">
    <div class='col-md-12'>
      <% if @results.model == Book %>
        <h2>Books search for "<%= @search_keyword %>"</h2>
        <%= render 'books/index', books: @results %>
      <% else %>
        <h2>Users search for "<%= @search_keyword %>"</h2>
        <%= render 'users/index', users: @results %>
      <% end %>
    </div>
  </div>
</div>